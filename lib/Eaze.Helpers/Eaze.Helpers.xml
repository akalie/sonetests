<actions>
    <action name="ProcessSearchFilter">
        <path>ProcessFilter</path>
        <parameters>
            <request>
                <param name="parameters">array( "search", "page", "order" )</param>
                <param name="excludedUrls">array( "add" )</param>
            </request>
        </parameters>
    </action>
    <action name="ProcessSearchFilterWithAdd">
        <path>ProcessFilter</path>
        <parameters>
            <request>
                <param name="parameters">array( "search", "page", "order" )</param>
                <param name="excludedUrls">array()</param>
            </request>
        </parameters>
    </action>
    <action name="DemoImageHelper">
        <path>demo/DemoImageHelper</path>
        <parameters>
            <request>
                <param name="tempPath">"temp://"</param>
            </request>
        </parameters>
    </action>
    <action name="GetMemcachedInfo">
        <redirects>
            <redirect name="success" path="vt://modules/memcached" />
        </redirects>
    </action>
</actions>